{
  "name": "secunik-logx-frontend",
  "version": "1.0.0",
  "description": "SecuNik LogX - Local-first Digital Forensics Platform Frontend",
  "type": "module",
  "private": true,
  "homepage": ".",
  "keywords": [
    "digital-forensics",
    "log-analysis",
    "cybersecurity",
    "threat-detection",
    "local-first",
    "forensics-platform"
  ],
  "author": "SecuNik Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --host localhost --port 5173 --open",
    "build": "tsc && vite build",
    "build:production": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview --host localhost --port 4173",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist node_modules/.vite",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "analyze": "npx vite-bundle-analyzer",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "zustand": "^4.4.7",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "@monaco-editor/react": "^4.6.0",
    "recharts": "^2.8.0",
    "@microsoft/signalr": "^8.0.7",
    "axios": "^1.6.2",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "date-fns": "^2.30.0",
    "react-syntax-highlighter": "^15.5.0",
    "@types/react-syntax-highlighter": "^15.5.11"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "eslint": "^8.53.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "@types/node": "^20.9.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "rimraf": "^5.0.5",
    "vitest": "^0.34.6",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "jsdom": "^22.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "vite-plugin-pwa": "^0.16.7",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/secunik/logx.git"
  },
  "bugs": {
    "url": "https://github.com/secunik/logx/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "volta": {
    "node": "18.18.2",
    "npm": "9.8.1"
  },
  "packageManager": "npm@9.8.1"
}